@model List<dentexpert.Models.About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<section id="content">
    <div class="container">
        <div class="row">
            <section id="inner-headline">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12">
                            <ul class="breadcrumb">
                                <li><a href="/Home/Index"><i class="fa fa-home"></i></a><i class="icon-angle-right"></i></li>
                                <li class="active">Hakkımızda</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </section>
            <div class="col-lg-12">
                
                <div class="clearfix">
                </div>
                <div class="row">
                    @foreach (var item in Model)
                    {
                        <article class="aligncenter">
                            <div class="">
                                <div class="post-heading">
                                    <h3 class="text-center">@item.title</h3>
                                </div>
                                <img src="@item.image" alt="" />
                            </div>
                            <p>
                                @item.description
                            </p>

                        </article>
                    }

                </div>
            </div>
        </div>
    </div>
</section>

